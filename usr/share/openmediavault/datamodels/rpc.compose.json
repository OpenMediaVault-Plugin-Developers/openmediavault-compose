  {
    "type": "rpc",
    "id": "rpc.compose.setjob",
    "params": {
      "type": "object",
      "properties": {
        "uuid": {
          "type": "string",
          "required": true
        },
        "enable": {
          "type": "boolean",
          "required": true
        },
        "filter": {
          "type": "string",
          "required": true
        },
        "backup": {
          "type": "boolean",
          "required": true
        },
        "prebackup": {
          "type": "string",
          "required": true
        },
        "postbackup": {
          "type": "string",
          "required": true
        },
        "maintenance": {
          "type": "boolean",
          "required": true
        },
        "cstate": {
          "type": "boolean",
          "required": true
        },
        "update": {
          "type": "boolean",
          "required": true
        },
        "prune": {
          "type": "boolean",
          "required": true
        },
        "filestart": {
          "type": "boolean",
          "required": true
        },
        "filestop": {
          "type": "boolean",
          "required": true
        },
        "sendemail": {
          "type": "boolean",
          "required": true
        },
        "verbose": {
          "type": "boolean",
          "required": true
        },
        "comment": {
          "type": "string"
        },
        "execution": {
          "type": "string",
          "enum": [
            "exactly",
            "hourly",
            "daily",
            "weekly",
            "monthly",
            "yearly",
            "reboot"
          ],
          "required": true
        },
        "minute": {
          "type": "array",
          "items": {
            "type": "string",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "*"
                ]
              },
              {
                "type": "string",
                "pattern": "^[0-9]|[1-5][0-9]$"
              }
            ]
          },
          "required": true
        },
        "everynminute": {
          "type": "boolean",
          "required": true
        },
        "hour": {
          "type": "array",
          "items": {
            "type": "string",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "*"
                ]
              },
              {
                "type": "string",
                "pattern": "^[0-9]|1[0-9]|2[0-3]$"
              }
            ]
          },
          "required": true
        },
        "everynhour": {
          "type": "boolean",
          "required": true
        },
        "month": {
          "type": "array",
          "items": {
            "type": "string",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "*"
                ]
              },
              {
                "type": "string",
                "pattern": "^[1-9]|1[0-2]$"
              }
            ]
          },
          "required": true
        },
        "dayofmonth": {
          "type": "array",
          "items": {
            "type": "string",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "*"
                ]
              },
              {
                "type": "string",
                "pattern": "^[1-9]|[12][0-9]|3[01]$"
              }
            ]
          },
          "required": true
        },
        "everyndayofmonth": {
          "type": "boolean",
          "required": true
        },
        "dayofweek": {
          "type": "array",
          "items": {
            "type": "string",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "*"
                ]
              },
              {
                "type": "string",
                "pattern": "^[1-7]$"
              }
            ]
          },
          "required": true
        }
      }
    }
  }
